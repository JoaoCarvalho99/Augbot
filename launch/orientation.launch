<launch>


    <node name="oriPiPico" pkg="Augbot" type="ori" output="screen">
        <param name="port" value="/dev/ttyACM2" /> 
        <param name="sensor" value="pipico" /> <!-- micro:bit or pi:pico -->
    </node>


    <node name="oriMicro" pkg="Augbot" type="ori" output="screen">
        <param name="port" value="/dev/ttyACM0" /> 
        <param name="sensor" value="microbit" /> <!-- micro:bit or pi:pico -->
    </node>


</launch>
